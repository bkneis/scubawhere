{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es5",
    "module": "es6", // common js
    "sourceMap": true,
    "allowJs": true,
    "experimentalDecorators": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}